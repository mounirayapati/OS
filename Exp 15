#include <stdio.h>

int main() {
    int start, length, i;

    printf("Enter starting block: ");
    scanf("%d", &start);

    printf("Enter length of file: ");
    scanf("%d", &length);

    printf("File blocks allocated:\n");
    for (i = start; i < start + length; i++) {
        printf("%d ", i);
    }

    return 0;
}

#include <stdio.h>

int main() {
    char user1[2][10] = {"u1file1", "u1file2"};
    char user2[2][10] = {"u2file1", "u2file2"};
    int i;

    printf("User 1 files:\n");
    for (i = 0; i < 2; i++)
        printf("%s\n", user1[i]);

    printf("\nUser 2 files:\n");
    for (i = 0; i < 2; i++)
        printf("%s\n", user2[i]);

    return 0;
}

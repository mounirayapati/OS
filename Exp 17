#include <stdio.h>
#include <stdlib.h>

int main() {
    int queue[5] = {120, 45, 67, 89, 150};
    int head = 90, i;
    int total = 0;

    for (i = 0; i < 5; i++) {
        total += abs(queue[i] - head);
        head = queue[i];
    }

    printf("Total head movement = %d\n", total);
    return 0;
}
